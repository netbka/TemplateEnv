{
  "name": "javascript-development-environment",
  "version": "1.0.0",
  "description": "JavaScript development environment",
  "scripts": {
    "start": "npm-run-all --parallel security-check open:devServer",
    "open:devServer": "babel-node buildScripts/devServer.js",
    "security-check": "nsp check",
    "prebuild": "rimraf ./dist && mkdir dist",
    "build": "babel-node buildScripts/build.js",
    "postbuild": "babel-node buildScripts/distServer.js",
    "deploy": "surge ./dist"
  },
  "author": "Cory House",
  "license": "MIT",
  "dependencies": {
    "vue": "^2.5.8",
    "whatwg-fetch": "2.0.3"
  },
  "devDependencies": {
    "babel-cli": "6.26.0",
    "babel-core": "6.26.0",
    "babel-loader": "7.1.2",
    "babel-preset-latest": "6.24.1",
    "babel-register": "6.26.0",
    "chai": "4.1.2",
    "chalk": "2.3.0",
    "cheerio": "0.22.0",
    "compression": "1.7.1",
    "cross-env": "5.1.1",
    "css-loader": "0.28.7",
    "eslint": "4.12.0",
    "eslint-plugin-import": "2.8.0",
    "eslint-watch": "3.1.3",
    "express": "4.16.2",
    "extract-text-webpack-plugin": "3.0.2",
    "html-webpack-plugin": "2.30.1",
    "jsdom": "11.4.0",
    "json-schema-faker": "0.4.6",
    "json-server": "0.12.1",
    "localtunnel": "1.8.3",
    "mocha": "4.0.1",
    "nock": "9.1.3",
    "npm-run-all": "4.1.2",
    "nsp": "3.1.0",
    "numeral": "2.0.6",
    "open": "0.0.5",
    "rimraf": "2.6.2",
    "style-loader": "0.19.0",
    "webpack": "3.8.1",
    "webpack-dev-middleware": "1.12.1",
    "webpack-hot-middleware": "2.20.0",
    "webpack-md5-hash": "0.0.5",
    "moment": "2.19.2"
  }
}

{
	"root": true,
	"extends": [
	  "eslint:recommended",
	  
	  "plugin:import/warnings"
	],
	"parser": "babel-eslint",
	"parserOptions": {
	  "ecmaVersion": 7,
	  "sourceType": "module"
	},
	"env": {
	  "browser": true,
	  "node": true,
	  "mocha": true,
	  "jquery": true
	},
	
	"rules": {
  		"no-console":"off",
  		"no-mixed-spaces-and-tabs":"off"
	},
	"plugins": [
		"html"
	]
  }
